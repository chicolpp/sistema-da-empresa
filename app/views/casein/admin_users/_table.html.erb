<!-- Scaffolding generated by Casein v5.1.1.1 -->

<table class="table table-striped table-bordered datatable" cellspacing="0" width="100%">
	<thead>
		<tr>
			<th>Nome de Usuário</th>
			<th>E-mail</th>
			<th>Pós Venda?</th>
			<th>Perfil de Acesso</th>
			<th data-orderable="false">&nbsp;</th>
		</tr>
	</thead>
	
	<tbody>
		<% users.each do |user| %>
			<tr>
				<td><%= casein_table_cell_link user.login, casein_admin_user_path(user) %></td>
				<td><%= casein_table_cell_link user.email, casein_admin_user_path(user) %></td>
				<td><%= casein_table_cell_link user.pos_venda ? "Sim" : "Não", casein_admin_user_path(user) %></td>
				<td><%= casein_table_cell_link casein_get_access_level_text(user.access_level), casein_admin_user_path(user) %></td>
				<td>
					<%= link_to(casein_show_row_icon("zoom-in"), casein_admin_user_path(user), class: "icon-table") %>
					<%= link_to(casein_show_row_icon("edit"), edit_casein_admin_user_path(user), class: "icon-table", "data-no-turbolinks" => true) %>
					<%= link_to(casein_show_row_icon("trash"), casein_admin_user_path(user), class: "icon-table", :method => :delete, :data => { :confirm => "Você tem certeza que deseja apagar este registro?" }) %>
				</td>
			</tr>
		<% end %>
	</tbody>
</table>